{
  "name": "prettier-plugin-angular-module-sort-metadata",
  "version": "0.0.1",
  "description": "prettier plugin to sort angular imports",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "lint": "prettier --check \"src/**/*.ts\"",
    "example": "yarn build && prettier --write --plugin . test/example.ts",
    "test": "yarn build && jest"
  },
  "dependencies": {
    "prettier": "2.8.3"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/prettier": "2.7.2",
    "jest": "^29.4.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
